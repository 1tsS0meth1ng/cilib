<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE simulator [
  <!ATTLIST algorithm id ID #IMPLIED>
  <!ATTLIST problem id ID #IMPLIED>
  <!ATTLIST measurements id ID #IMPLIED>
]>

<simulator>

    <algorithms>
        <algorithm id="gcpso-gbest" class="PSO.GCPSO">
	  <topology class="PSO.GBestTopology" />
          <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations"/>
        </algorithm>
	<algorithm id="gcpso-lbest" class="PSO.GCPSO">
	  <topology class="PSO.LBestTopology" />
      <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations" />
	</algorithm>
	<algorithm id="gcpso-vn" class="PSO.GCPSO">
	  <topology class="PSO.VonNeumannTopology" />
	  <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations" />
	</algorithm>
	<algorithm id="pso-gbest" class="PSO.PSO">
	  <topology class="PSO.GBestTopology" />
	  <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations" />
	</algorithm>
	<algorithm id="pso-lbest" class="PSO.PSO">
	  <topology class="PSO.LBestTopology" />
	  <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations" />
	</algorithm>
	<algorithm id="pso-vn" class="PSO.PSO">
	  <topology class="PSO.VonNeumannTopology" />
	  <addStoppingCondition class="StoppingCondition.MaximumFitnessEvaluations" />
	</algorithm>
    </algorithms>
    
    <problems>
        <problem id="spherical" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Spherical" />
        </problem>
        <problem id="quadric" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Quadric" />
        </problem>
        <problem id="rosenbrock" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Rosenbrock" />
        </problem>
        <problem id="ackley" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Ackley" />
        </problem>
        <problem id="griewank" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Griewank" />
        </problem>
        <problem id="rastrigin" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Rastrigin" />
        </problem>
        <problem id="schwefel" class="Problem.FunctionMinimisationProblem">
            <function class="Functions.Schwefel" />
        </problem>
    </problems>

    <measurements id="measurements" class="Simulator.MeasurementSuite" samples="30" resolution="100">
        <addMeasurement class="Measurement.FitnessEvaluations" />
        <addMeasurement class="Measurement.FunctionMinimisationError" />
    </measurements>
    
    <simulations>

        <simulation>
            <algorithm idref="gcpso-gbest" />
            <problem idref="spherical" />
            <measurements idref="measurements" file="spherical-gcpso-gbest.txt" />
        </simulation>
        <simulation>
            <algorithm idref="gcpso-lbest" />
            <problem idref="spherical" />
            <measurements idref="measurements" file="spherical-gcpso-lbest.txt" />
        </simulation>
        <simulation>
            <algorithm idref="gcpso-vn" />
            <problem idref="spherical" />
            <measurements idref="measurements" file="spherical-gcpso-vn.txt" />
        </simulation>
        <simulation>
            <algorithm idref="pso-gbest" />
            <problem idref="spherical" />
            <measurements idref="measurements" file="spherical-pso-gbest.txt" />
        </simulation>
